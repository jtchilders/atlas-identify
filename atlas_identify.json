      "steps_per_valid":               1
   },
   "tensorboard": {
      "log_dir":                       "./logs",
      "histogram_freq":                1,
      "write_graph":                   true,
      "write_images":                  true,
      "write_grads":                   true,
      "embeddings_freq":               0
   }
